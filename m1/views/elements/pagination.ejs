<% if(totalRecords >= 1) { %>
<div class="span6">
	<!-- <div class="dataTables_info">
		Showing <%= startPage %> to <%= endPage %> of <%= totalRecords %> entries
	</div> -->
	<% if(pageCount > 1) { %>
		<ul class="pagination">
			<% if(currentPage > 1) { %>
				<li><a href="<%= currentUrl %>page=1">&laquo;</a></li>
			<% } %>
			<% var i = 1; %>
			<% if(currentPage > 5) { %>
				<% i = currentPage - 4; %>
			<% } %>

			<% if(i !== 1) { %>
				<li class="disabled"><a href="#">...</a></li>
			<% } %>
			<% while(i < pageCount) { %>
				<% if(currentPage == i) { %>
					<li class="active"><a href="<%= currentUrl %>page=<%= i %>"><%= i %></a></li>
				<% } else { %>
					<li><a href="<%= currentUrl %>page=<%= i %>"><%= i %></a></li>
				<% } %>
				
				<% if(i == (currentPage + 4)) { %>
					<li class="disabled"><a href="<%= i %>"><%= i %></a></li>
					<% break; %>
				<% } %>
				<% i++; %>
			<% } %>
			
			<% if(currentPage != pageCount) { %>
				<li><a href="<%= currentUrl %>page=<%= pageCount %>">&raquo;</a></li>
			<% } %>
		</ul>
	<% } %>
</div>
<% } %>