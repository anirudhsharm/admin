<% layout('layouts/admin') %>
<div class="col-md-12">
    <form class="form-horizontal" method="post" role="form" action="" name="daatsetForm" id="daatsetForm" enctype="multipart/form-data">
        <div class="card ">
            <div class="card-header">
                <% include ../elements/flash.ejs %>
                <a href="<%= admin_url %>dataset" class="btn btn-wd btn-info btn-outline pull-right">
                    <span class="btn-label">
                        <i class="fa fa-file-text"></i>
                    </span>
                    Contact List
                </a>
            </div>
            <div class="card-body ">
                <div class="row">
                    <label class="col-sm-2 col-form-label">File <star class="star">*</star></label>
                    <div class="col-sm-7">
                        <div class="form-group">
                            <input class="form-control" type="file" name="file" accept=".csv" required="true" placeholder="Upload file" />
                        </div>
                    </div>
                </div>
            </div>
            <div class="card-footer text-center">
                <button type="submit" class="btn btn-info" id = "submitBtn">Upload</button>
            </div>
        </div>
    </form>
</div>
<script>
    $(document).ready(function() {
        App.validateUploadDatasetPage();
    })
</script>